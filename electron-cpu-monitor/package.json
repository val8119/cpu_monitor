{
  "name": "electron-cpu-monitor",
  "productName": "CPU Monitor",
  "version": "1.0.0",
  "description": "Monitor your CPU usage as well as RAM",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder"
  },
  "build": {
    "appId": "electron-cpu-monitor",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/favicon.ico"
    },
    "nsis": {
      "installerIcon": "src/favicon.ico",
      "uninstallerIcon": "src/favicon.ico",
      "uninstallDisplayName": "CPU Monitor",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "val8119",
    "email": "48407846+val8119@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_cpu_monitor"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "modclean": "^3.0.0-beta.1",
    "os-utils": "0.0.14"
  },
  "devDependencies": {
    "electron-builder": "^22.9.1",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "11.2.1"
  }
}
